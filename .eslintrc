{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 2],
    "eol-last": "off",
    "class-methods-use-this": "off",
    "import/no-unresolved": "off",
    "no-plusplus": "off"
  },
  "extends": ["airbnb-base"]
}